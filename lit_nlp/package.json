{
  "name": "lit-client",
  "version": "1.0.0-rc1",
  "description": "Client app for LIT, the Learning Interpretability Tool",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "clean": "rm -rf ./client/build",
    "lint": "eslint -c ./.eslint.json 'client/**/*.ts'",
